#include <iostream>

using namespace std;

	int main()
{
	float a, b, P; 
	
	cout << "Podaj liczbe " <<endl <<endl;
	cin >> P; 
	// P = [TUTAJ WPISZ LICZBE]; // <-- tutaj wpisz liczbe jaka chcesz spierwiastkowac, usun cina jesli tutaj ja okreslasz

	a = P;
	b = 1;
	
	for(int i=0; i<10; i++)
	{
		a = (a+b)/2;
		b = P/a;
	}
	
	cout << a <<endl <<b;
	
	return 0;
}
